        <form class="suord" id="purc" method="post" name="form1" action="/order/add.shtml" onsubmit="return validateform(this);">
            <p><label>姓名：</label><input type="text" name="username" id="username" value=""></p>
            <p><label>手机号码：</label><input type="text" name="phone" id="phone" value=""></p>
            <p><label>省份：</label>
                <select name="city" id="city" onChange="selectcityarea('city', 'area', 'form1');">
                    <option value="0" selected>选择省份</option>
                </select>
            </p>
            <p><label>县/市：</label>
                <select name="area" id="area">
                    <option value="0" selected>选择城市</option>
                </select>
            </p>
            <p><label>详细地址：</label><textarea id="address" name="address"></textarea></p>
            <p><label>留言：</label><textarea id="message" name="message"></textarea></p>
            <input type="hidden" name="token" value="<{$token}>">
            <input type="hidden" name="secretkey" value="<{$secretkey}>">
            <input type="hidden" name="productid" value="<{$productid}>">
            <input type="hidden" name="act" value="add">
            <input type="submit" name="submit" id="submit" value="立即提交订单">
        </form>
            
