        <script type="text/javascript">
        $(function(){
            var $_pid;
            $('.perma div a').click(function(){
                var ind=$(this).index();
                $('.perma div a').removeClass('curr');
                $(this).addClass('curr');
                $('.funct').find('.lonti').removeClass('shay').addClass('disn');
                $('.funct').find('.lonti').eq(ind).removeClass('disn').addClass('shay');
            });	
            var bol=true;
            $('.price span').click(function(){
                if($(this).hasClass('check')){
                    $(this).removeClass('check');
                }	
                else{
                    $('.price span').removeClass('check');
                    $(this).addClass('check');
                    $_pid = $(this).attr('data-pid');
                    $(".suord input[name='productid']").val($_pid);
                }
            });
        });
        </script>
